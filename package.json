{
	"name" : "@oktarintentakel/annex",
	"version" : "0.1.7-beta",
	"author" : "Sebastian Schlapkohl",
	"description" : "Unit-tested and semantically packaged standard solutions for everyday frontend tasks and syntax streamlining for those ugly darks corners of JS.",
	"license" : "MIT",
	"repository" : {
		"type" : "git",
		"url" : "git+https://github.com/OktarinTentakel/annex.git"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"private" : false,
	"keywords" : [
		"frontend helpers",
		"basic type checks",
		"basic value checks",
		"data structure helpers",
		"dom handling",
		"string operations",
		"randomization",
		"better timers",
		"events",
		"requests",
		"browser context evaluation",
		"interaction helpers",
		"navigation helpers",
		"viewport helpers",
		"better logging",
		"cookie handling"
	],

	"type" : "module",
	"sideEffects" : false,
	"directories" : {
		"docs" : "./docs/documentation",
		"examples" : "./docs/examples",
		"test" : "./test"
	},

	"__comment_on_browserslist" : "only relevant for building the ES5 monolith",
	"browserslist" : ["IE >= 11"],

	"__comment_on_ava" : "updating to v4 requires full refactoring of unit tests",
	"devDependencies" : {
		"ava" : "~3.15.0",
		"@babel/core" : "~7.20.7",
		"@babel/preset-env" : "~7.20.2",
		"browser-env" : "~3.3.0",
		"core-js" : "~3.26.1",
		"cors" : "^2.8.5",
		"express" : "^4.18.2",
		"gulp" : "^4.0.2",
		"gulp-connect" : "^5.7.0",
		"gulp-inject-string" : "^1.1.2",
		"gulp-shell" : "^0.8.0",
		"gulp-sourcemaps" : "^3.0.0",
		"gulp-terser" : "^2.1.0",
		"jsdoc" : "^4.0.2",
		"natives" : "^1.1.6",
		"rollup" : "^3.8.1",
		"@rollup/plugin-babel" : "^6.0.3",
		"@rollup/plugin-commonjs" : "^24.0.0",
		"@rollup/plugin-node-resolve" : "^15.0.1",
		"@rollup/plugin-terser" : "^0.2.1",
		"rollup-plugin-includepaths" : "^0.2.4",
		"serve-static" : "^1.15.0",
		"st" : "^3.0.0",
		"yargs" : "^17.6.2"
	},

	"scripts" : {
		"test" : "ava --color",
		"test-dist" : "SOURCE=dist ava --color",
		"test-es5-monolith" : "SOURCE=es5-monolith ava --color"
	}
}
